{ 
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb-base"],
  "plugins": ["prettier", "react"], 
  "rules": {
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }], //선언한 변수 안 쓰면 에러 낼지 
    "prettier/prettier": "off", //프리티어와 관련된 에러, prettier 설정을 따로해서 사용하므로 지금은 꺼놨다.
    "import/no-unresolved": "off", //내가 만든 모듈 가져오기 쉽게 하는 방법
    "import/extensions": "off", //확장자 안 붙여도 에러 안나게 설정
    "no-undef" : "off",
    "import/no-extraneous-dependencies": "warn",
    "max-len": "off",
    "space-infix-ops": "off",
    "object-curly-newline": "off",
    "no-use-before-define": "off",
    "import/prefer-default-export": "off",
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off",
    "no-shadow": "off",
    "no-confusing-arrow": "off",
    "indent": "off",
    "function-paren-newline": "off",
    "no-continue": "off",
    "no-param-reassign": "off",
    "no-return-await": "off"
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"] 
      }
    }
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  }
}